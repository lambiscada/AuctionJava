package es.udc.is.isg019.subastador.model.auctionservice;

import java.util.List;

import es.udc.is.isg019.subastador.model.puja.Puja;

public class PujasBlock {

	private List<Puja> pujas;
	private boolean existMorePujas;

	public PujasBlock(List<Puja> pujas, boolean existMorePujas) {
		super();
		this.pujas = pujas;
		this.existMorePujas = existMorePujas;
	}

	public List<Puja> getPujas() {
		return pujas;
	}

	public boolean getExistMorePujas() {
		return existMorePujas;
	}

}
