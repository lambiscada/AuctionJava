package es.udc.is.isg019.subastador.model.userservice;

@SuppressWarnings("serial")
public class IncorrectNumeroTarjetaBancoException extends Exception {

    private Long numero;

    public IncorrectNumeroTarjetaBancoException(Long numero) {
        super("Incorrect numero tarjeta exception => numero = " + numero);
        this.numero = numero;
    }

    public Long getNumero() {
        return numero;
    }


}
